{
    "env": {
        "browser": true,
        "commonjs": true,
        "amd": true,
        "es6": true,
        "node": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "script"
    },
    "rules": {
        "no-var": "warn",
        "no-extra-semi": "warn",
        "prefer-template": "warn",
        "func-style": "warn",
        "semi": "error",
        "quotes": ["error", "single"],
        "no-const-assign": "error",
        "no-undef": "error",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}